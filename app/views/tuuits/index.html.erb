<p id="notice"><%= notice %></p>

<h1>Tuuits</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>User</th>
      <th>Time</th>
      <th>Tuuit</th>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tuuits.each do |tuuit| %>
      <tr>
        <td><%= tuuit.id %></td>
        <td><%= tuuit.user_id %></td>
        <td><%= tuuit.time %></td>
        <td><%= tuuit.tuuit %></td>
        <td><%= tuuit.title %></td>
        <td><%= link_to 'Show', tuuit %></td>
        <td><%= link_to 'Edit', edit_tuuit_path(tuuit) %></td>
        <td><%= link_to 'Destroy', tuuit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tuuit', new_tuuit_path %>
